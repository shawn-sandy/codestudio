{
  "name": "codestudio",
  "publisher": "shawnsandy",
  "displayName": "CodeStudio Extension Pack",
  "description": "VSCODE extension for UI/UX (frontend) development",
  "repository": {
    "type": "git",
    "url": "https://github.com/shawn-sandy/codestudio"
  },
  "galleryBanner": {
    "color": "#1E1E1E",
    "theme": "dark"
  },
  "icon": "logo.png",
  "scripts": {
    "np:major": "np major --yolo=true --no-publish",
    "np:minor": "np minor --yolo=true --no-publish",
    "np:patch": "np patch --yolo=true --no-publish",
    "build": "vsce package",
    "changelog": "auto-changelog",
    "history": "auto-changelog --output HISTORY.md"
  },
  "keywords": [
    "VSCODE",
    "Adobe XD",
    "Extension pack",
    "GitLens",
    "Auto close tags",
    "Auto rename tags",
    "Vetur",
    "Indent Rainbow",
    "Markdown preview enhanced",
    "One dark pro",
    "Git ignore",
    "Bracket Pair Colorizer"
  ],
  "author": "shawnsandy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shawn-sandy/codestudio/issues"
  },
  "homepage": "https://github.com/shawn-sandy/codestudio",
  "version": "0.6.2",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Extension Packs"
  ],
  "extensionPack": [
    "abusaidm.html-snippets",
    "capaj.vscode-standardjs-snippets",
    "christian-kohler.npm-intellisense",
    "CoenraadS.bracket-pair-colorizer",
    "DavidAnson.vscode-markdownlint",
    "dbaeumer.vscode-eslint",
    "eamodio.gitlens",
    "ecmel.vscode-html-css",
    "esbenp.prettier-vscode",
    "EditorConfig.EditorConfig",
    "eg2.vscode-npm-script",
    "mkaufman.HTMLHint",
    "GitHub.vscode-pull-request-github",
    "mrmlnc.vscode-scss",
    "humao.rest-client",
    "HookyQR.beautify",
    "kenhowardpdx.vscode-gist",
    "leizongmin.node-module-intellisense",
    "mervin.markdown-formatter",
    "mgmcdermott.vscode-language-babel",
    "michelemelluso.gitignore",
    "oderwat.indent-rainbow",
    "pflannery.vscode-versionlens",
    "shd101wyy.markdown-preview-enhanced",
    "sidthesloth.html5-boilerplate",
    "sleistner.vscode-fileutils",
    "streetsidesoftware.code-spell-checker",
    "tombonnike.vscode-status-bar-format-toggle",
    "VisualStudioExptTeam.vscodeintellicode",
    "vincaslt.highlight-matching-tag",
    "xabikos.JavaScriptSnippets",
    "yzhang.markdown-all-in-one"
  ],
  "dependencies": {
    "vsce": "^1.94.0"
  },
  "devDependencies": {
    "auto-changelog": "^2.2.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "np": "^6.4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
